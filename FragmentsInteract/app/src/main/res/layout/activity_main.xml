<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sambovisal.fragmentsinteract.MainActivity">

    <fragment
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:id="@+id/fragment"
        android:name="com.example.sambovisal.fragmentsinteract.TopFragClass"/>
    <fragment
        android:layout_width="match_parent"
        android:layout_height="210dp"
        android:id="@+id/fragment2"
        android:layout_below="@+id/fragment"
        android:name="com.example.sambovisal.fragmentsinteract.BottomFragClass"/>
</RelativeLayout>
